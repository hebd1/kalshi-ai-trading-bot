# Docker Environment Configuration Example
# Copy this file to .env and fill in your actual values

# DockerHub Configuration
DOCKER_USERNAME=yourusername

# Kalshi Demo/Test API Configuration
KALSHI_API_KEY=your_demo_api_key_here
KALSHI_BASE_URL=https://demo-api.kalshi.co

# Kalshi Production API Configuration (for live trading)
KALSHI_API_KEY_PROD=your_prod_api_key_here
KALSHI_BASE_URL_PROD=https://api.elections.kalshi.com

# XAI (Grok) API Configuration
XAI_API_KEY=your_xai_api_key_here

# OpenAI API Configuration (optional)
OPENAI_API_KEY=your_openai_api_key_here

# Timezone
TZ=America/Denver

# Version Tag (optional)
VERSION=latest
